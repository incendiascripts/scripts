local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("marvel game script", "Synapse")





local Tab = Window:NewTab("characters")
local Section = Tab:NewSection("characters")


Section:NewButton("delete backround", "ButtonInfo", function()
    game.ReplicatedStorage.Ui.MainScreen:destroy()
       game.WakSpace.Selection_Background:destroy()
end)


Section:NewButton("be dark josie", "ButtonInfo", function()
    game.ReplicatedStorage.Remotes.CharacterSelection:InvokeServer("Custom", "Dark Josie")
end)



Section:NewButton("be prom gwen", "ButtonInfo", function()
    game.ReplicatedStorage.Remotes.CharacterSelection:InvokeServer("Custom", "Prom Gwen")
end)


Section:NewButton("be freya", "ButtonInfo", function()
    game.ReplicatedStorage.Remotes.CharacterSelection:InvokeServer("Custom", "Freya Mikaelson")
end)



Section:NewButton("be barbie", "ButtonInfo", function()
    game.ReplicatedStorage.Remotes.CharacterSelection:InvokeServer("Custom", "Barbie")
end)



Section:NewButton("be Scarlet witch", "ButtonInfo", function()
    game.ReplicatedStorage.Remotes.CharacterSelection:InvokeServer("Custom", "Scarlet witch: Multiverse Of Madness")
end)




Section:NewButton("be sabrina", "ButtonInfo", function()
    game.ReplicatedStorage.Remotes.CharacterSelection:InvokeServer("Custom", "Sabrina")
end)


Section:NewButton("be child", "ButtonInfo", function()
    game.ReplicatedStorage.Remotes.CharacterSelection:InvokeServer("Custom", "Child")
end)




Section:NewButton("be white raven", "ButtonInfo", function()
    game.ReplicatedStorage.Remotes.CharacterSelection:InvokeServer("Custom", "White Raven")
end)



Section:NewButton("be madison", "ButtonInfo", function()
    game.ReplicatedStorage.Remotes.CharacterSelection:InvokeServer("Custom", "Madison Montgomery")
end)
