local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("all my scripts", "Synapse")





local Tab = Window:NewTab("scripts")
local Section = Tab:NewSection("scripts")




Section:NewButton("caos script", "ButtonInfo", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/incendiascripts/scripts/main/caos%20roblox%20script'),true))()
end)



Section:NewButton("charmed script", "ButtonInfo", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/incendiascripts/scripts/main/charmed'),true))()
end)




Section:NewButton("crossover dynasty", "ButtonInfo", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/incendiascripts/scripts/main/crossover%20dynasty'),true))()
end)




Section:NewButton("gen v marvel", "ButtonInfo", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/incendiascripts/scripts/main/marvel%20gen%20v%3F'),true))()
end)






Section:NewButton("marvel liberty", "ButtonInfo", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/incendiascripts/scripts/main/marvel%20liberty%20script'),true))()
end)






Section:NewButton("the world of marvel", "ButtonInfo", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/incendiascripts/scripts/main/the%20world%20of%20marvel'),true))()
end)







Section:NewButton("tvo", "ButtonInfo", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/incendiascripts/scripts/main/tvo%20script-self%20powers%20only'),true))()
end)





Section:NewButton("twp script", "ButtonInfo", function()
