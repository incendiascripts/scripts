local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("marvel game script", "Synapse")





local Tab = Window:NewTab("keybinds and more")
local Section = Tab:NewSection("keybinds")


Section:NewButton("destroy backround", "ButtonInfo", function()
    game.ReplicatedStorage.Ui.MainScreen:destroy()
       game.WakSpace.Selection_Background:destroy()
end)

Section:NewButton("be white phoenix", "ButtonInfo", function()
    local args = {
    [1] = "WhitePhoenix"
}

game:GetService("ReplicatedStorage"):WaitForChild("ChangePlayerCharacter"):FireServer(unpack(args))

end)


Section:NewButton("be wiccan", "ButtonInfo", function()
    local args = {
    [1] = "Wiccan"
}

game:GetService("ReplicatedStorage"):WaitForChild("ChangePlayerCharacter"):FireServer(unpack(args))

end)
